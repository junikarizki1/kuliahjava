package javaapplication4;

/**
 *
 * @author Mahasiswa
 */
public class main {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        int angka1, angka2, hasil1, hasil2, hasil3, hasil4;
        
        angka1 = 2;
        angka2 = 3;
        hasil1 = angka1 + angka2;
        hasil2 = angka1 - angka2;
        hasil3 = angka1 * angka2;
        hasil4 = angka1 / angka2;
        
        System.out.println(angka1 + " + " + angka2 + " = " +hasil1);
        System.out.println(angka1 + " - " + angka2 + " = " +hasil2);
        System.out.println(angka1 + " * " + angka2 + " = " +hasil3);
        System.out.println(angka1 + " : " + angka2 + " = " +hasil4);
    }
    
}
